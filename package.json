{
  "name": "@stackspot/cdk-env-sns",
  "version": "0.1.2",
  "description": "Component to manage SNS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii --verbose --fail-on-warnings",
    "build:watch": "npm run build -- --watch",
    "lint": "eslint --ext=ts --max-warnings=0 .",
    "lint:fix": "npm run lint -- --fix",
    "package:dotnet": "jsii-pacmak --target=dotnet --verbose --validate-assemblies",
    "package:js": "jsii-pacmak --target=js --verbose --validate-assemblies",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "author": {
    "name": "StackSpot",
    "email": "contact@stackspot.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stack-spot/sns-env-jsii-component.git"
  },
  "license": "MIT",
  "dependencies": {
    "aws-cdk-lib": "^2.10.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "aws-cdk-lib": "^2.10.0",
    "constructs": "^10.0.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^26.4.2",
    "jsii": "^1.54.0",
    "jsii-pacmak": "^1.54.0",
    "ts-jest": "^26.2.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.10.0",
    "constructs": "^10.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true
  },
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "full",
    "targets": {
      "dotnet": {
        "namespace": "StackSpot.Env.Sns",
        "packageId": "StackSpot.Env.Sns",
        "iconUrl": "https://www.nuget.org/profiles/stackspot/avatar"
      }
    }
  }
}
